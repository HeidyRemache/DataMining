#Librerías para Cluster
import matplotlib.pyplot as plt
import numpy as np
from sklearn.cluster import DBSCAN

url = 'https://raw.githubusercontent.com/edisonmalan/Edison-Malan-Patients/main/casasExam.csv'
datos=np.loadtxt(url,delimiter=",")
datos

clusters=DBSCAN(eps=2,min_samples=10).fit_predict(datos)
print(clusters) 

(figsize=(8,8))
plt.scatter(datos[:,0],datos[:,1],c=clusters,s=100)
plt.xlabel("Años de la construccion de las casa")
plt.ylabel("Precio")
plt.box(False)
